#: main.cpp:24 main.cpp:28
#, kde-format
msgid "Oxygen Settings"
msgstr "Oxygen ayarları"

#: oxygenanimationconfigwidget.cpp:34
#, kde-format
msgid "Focus, mouseover and widget state transition"
msgstr "Fokus, kursorla işarələmə və vidjet rejiminə keçid"

#: oxygenanimationconfigwidget.cpp:35
#, kde-format
msgid ""
"Configure widgets' focus and mouseover highlight animation, as well as "
"widget enabled/disabled state transition"
msgstr ""
"Vidjetləri fokuslamaq və siçanla işarələyərək vurğulamaq animasiyasını "
"tınzimləmək, həmçinin vidjetləri aktiv etmək/söndürmək vəziyyətləri arası "
"keçid"

#: oxygenanimationconfigwidget.cpp:39
#, kde-format
msgid "Toolbar highlight"
msgstr "Alətlər panelinə diqqət çəkmək"

#: oxygenanimationconfigwidget.cpp:39
#, kde-format
msgid "Configure toolbars' mouseover highlight animation"
msgstr "Alətlər panelinə kursorla diqqət çəkmək animasiaysını tənzimləmək"

#: oxygenanimationconfigwidget.cpp:44
#, kde-format
msgid "Menu bar highlight"
msgstr "Menyu çubuğuna diqqət çəkmək"

#: oxygenanimationconfigwidget.cpp:44
#, kde-format
msgid "Configure menu bars' mouseover highlight animation"
msgstr "Menyu çubuğuna kursorla diqqət çəkmək animasiaysını tənzimləmək"

#: oxygenanimationconfigwidget.cpp:47
#, kde-format
msgid "Menu highlight"
msgstr "Menyunu vurğulamaq"

#: oxygenanimationconfigwidget.cpp:47
#, kde-format
msgid "Configure menus' mouseover highlight animation"
msgstr "Manyuaya kursorla diqqət çəkmək animasiaysını tənzimləmək"

#: oxygenanimationconfigwidget.cpp:50
#, kde-format
msgid "Progress bar animation"
msgstr "Gedişat paneli animasiyası"

#: oxygenanimationconfigwidget.cpp:50
#, kde-format
msgid "Configure progress bars' steps animation"
msgstr "Gedişat paneli mərhələləri animasiyasını tənzimləmək"

#: oxygenanimationconfigwidget.cpp:53
#, kde-format
msgid "Tab transitions"
msgstr "Vərəqlər arası keçid"

#: oxygenanimationconfigwidget.cpp:53
#, kde-format
msgid "Configure fading transition between tabs"
msgstr "Vərəqlər arası solma effektli keçidi tənzimləmək"

#: oxygenanimationconfigwidget.cpp:57
#, kde-format
msgid "Label transitions"
msgstr "Keçidlər etiketi"

#: oxygenanimationconfigwidget.cpp:57
#, kde-format
msgid "Configure fading transition when a label's text is changed"
msgstr "Etiketin mətni dəyişdiyində solma effektli keçidi tənzimləmək"

#: oxygenanimationconfigwidget.cpp:61
#, kde-format
msgid "Text editor transitions"
msgstr "Mətn redaktoru keçidləri"

#: oxygenanimationconfigwidget.cpp:61
#, kde-format
msgid "Configure fading transition when an editor's text is changed"
msgstr "Redaktorun mətni dəyişdiyində solma effektli keçidi tənzimləmək"

#: oxygenanimationconfigwidget.cpp:65
#, kde-format
msgid "Combo box transitions"
msgstr "Birləşik qeyd qutuları arası keçid"

#: oxygenanimationconfigwidget.cpp:66
#, kde-format
msgid ""
"Configure fading transition when a combo box's selected choice is changed"
msgstr ""
"Birləşik qeyd qutunun seçilmiş deçimi dəyişdiyində solma effektli keçidi "
"tənzimləmək"

#: oxygenanimationconfigwidget.cpp:77
#, kde-format
msgid "Progress bar busy indicator"
msgstr "İşlək gedişat paneli indikatoru"

#: oxygenanimationconfigwidget.cpp:77
#, kde-format
msgid "Configure progress bars' busy indicator animation"
msgstr "İşlək gedişat paneli indikatoru animasiyasını tənzimləmək"

