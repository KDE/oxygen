#: main.cpp:24 main.cpp:28
#, kde-format
msgid "Oxygen Settings"
msgstr "Oksijen Ayarları"

#: oxygenanimationconfigwidget.cpp:34
#, kde-format
msgid "Focus, mouseover and widget state transition"
msgstr "Odak, fare ile üzerine gelme ve araç takımı durumu geçişi"

#: oxygenanimationconfigwidget.cpp:35
#, kde-format
msgid ""
"Configure widgets' focus and mouseover highlight animation, as well as "
"widget enabled/disabled state transition"
msgstr ""
"Araç takımı odaklanması ve fare ile üzerine gelinmesinde kullanılacak "
"canlandırmayı ve araç takımlarının etkin/devre dışı geçişlerini yapılandır"

#: oxygenanimationconfigwidget.cpp:39
#, kde-format
msgid "Toolbar highlight"
msgstr "Araç çubuğu vurgulaması"

#: oxygenanimationconfigwidget.cpp:39
#, kde-format
msgid "Configure toolbars' mouseover highlight animation"
msgstr ""
"Araç çubuklarını yapılandır'ın fare üzerine gelindiğindeki canlandırması"

#: oxygenanimationconfigwidget.cpp:44
#, kde-format
msgid "Menu bar highlight"
msgstr "Menü çubuğu vurgulaması"

#: oxygenanimationconfigwidget.cpp:44
#, kde-format
msgid "Configure menu bars' mouseover highlight animation"
msgstr ""
"Menü çubuklarının üzerine fare ile odaklanıldığında etkinleştirilecek "
"canlandırmayı yapılandır"

#: oxygenanimationconfigwidget.cpp:47
#, kde-format
msgid "Menu highlight"
msgstr "Menü vurgulaması"

#: oxygenanimationconfigwidget.cpp:47
#, kde-format
msgid "Configure menus' mouseover highlight animation"
msgstr ""
"Menülerin üzerine fare ile odaklanıldığında etkinleştirilecek canlandırmayı "
"yapılandır"

#: oxygenanimationconfigwidget.cpp:50
#, kde-format
msgid "Progress bar animation"
msgstr "İlerleme çubuğu canlandırması"

#: oxygenanimationconfigwidget.cpp:50
#, kde-format
msgid "Configure progress bars' steps animation"
msgstr "İlerleme çubuğu adım canlandırmasını yapılandır"

#: oxygenanimationconfigwidget.cpp:53
#, kde-format
msgid "Tab transitions"
msgstr "Sekme geçişleri"

#: oxygenanimationconfigwidget.cpp:53
#, kde-format
msgid "Configure fading transition between tabs"
msgstr "Sekmeler arası geçişte kullanılan soldurmayı yapılandır"

#: oxygenanimationconfigwidget.cpp:57
#, kde-format
msgid "Label transitions"
msgstr "Etiket geçişleri"

#: oxygenanimationconfigwidget.cpp:57
#, kde-format
msgid "Configure fading transition when a label's text is changed"
msgstr "Bir etiketin metni değiştirildiğinde kullanılan soldurmayı yapılandır"

#: oxygenanimationconfigwidget.cpp:61
#, kde-format
msgid "Text editor transitions"
msgstr "Metin düzenleyici geçişleri"

#: oxygenanimationconfigwidget.cpp:61
#, kde-format
msgid "Configure fading transition when an editor's text is changed"
msgstr ""
"Bir düzenleyicinin metni değiştirildiğinde kullanılan soldurmayı yapılandır"

#: oxygenanimationconfigwidget.cpp:65
#, kde-format
msgid "Combo box transitions"
msgstr "Çoklu kutu geçişleri"

#: oxygenanimationconfigwidget.cpp:66
#, kde-format
msgid ""
"Configure fading transition when a combo box's selected choice is changed"
msgstr ""
"Bir çoklu kutunun seçili tercihi değiştiğinde kullanılan soldurma geçişini "
"yapılandır"

#: oxygenanimationconfigwidget.cpp:77
#, kde-format
msgid "Progress bar busy indicator"
msgstr "İlerleme çubuğu meşgul göstergesi"

#: oxygenanimationconfigwidget.cpp:77
#, kde-format
msgid "Configure progress bars' busy indicator animation"
msgstr "İlerleme çubuğunun meşgul canlandırmasını yapılandır"

